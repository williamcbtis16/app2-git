                $(this).attr("href",twitterUrl);
            });
            $(document).on( 'click' , '#facebookAnchor' , function( e ){
                $(this).attr("href",facebookUrl);    
            });
            var nowTime = moment.tz(moment(), 'America/Mexico_City'),
